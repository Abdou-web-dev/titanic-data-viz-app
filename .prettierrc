{
  "parser": "typescript",
  "printWidth": 120,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "lf",
  "alignTernaryLines": false,
  "tabWidth": 2,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "generatorStarSpacing": true,
  "spaceBeforeFunctionParen": true,
  "jsxSingleQuote": false,
  "singleAttributePerLine": true,
  "jsxSingleAttributePerLine": false
}
